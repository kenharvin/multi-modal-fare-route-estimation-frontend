{
  "expo": {
    "name": "NaviGO",
    "slug": "multi-modal-fare-estimation",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "sdkVersion": "54.0.0",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#0B3A82"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.multimodalfare.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs your location to provide route recommendations."
      }
    },
    "android": {
      "package": "com.multimodalfare.app",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "INTERNET"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "5cf01d37-b7e5-4e7f-998f-09b9227abf15"
      }
    },
    "owner": "raikiri",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/5cf01d37-b7e5-4e7f-998f-09b9227abf15"
    }
  }
}
